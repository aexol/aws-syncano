{
  "license": "MIT",
  "scripts": {
    "bootstrap-dev": "sh -c 'cd ../simple-service-auth && yarn install --ignore-scripts --production=false'",
    "sync-aws-utils": "mkdir -p node_modules/aws-utils && ../simple-service-auth/node_modules/.bin/sync-files ../aws-utils node_modules/aws-utils",
    "sync-local": "npm run sync-aws-utils",
    "clean-aws-utils": "../simple-service-auth/node_modules/.bin/rimraf node_modules/aws-utils/node_modules",
    "clean-local": "npm run clean-aws-utils",
    "postinstall": "npm run bootstrap-dev && npm run sync-local && npm run clean-local"
  },
  "dependencies": {
    "aws-utils": "file:../aws-utils",
    "syncano-server": "^1.0.1-13"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "sync-files": "^1.0.3"
  }
}
