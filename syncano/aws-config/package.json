{
  "name": "aws-config",
  "version": "0.0.3",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:src && npm run build:env",
    "build:src": "sh ./bin/compile",
    "build:env": "sh ./bin/compile-env"
  },
  "dependencies": {
    "aws-bluebird": "^1.0.0",
    "semver": "^5.4.1",
    "syncano-server": "beta"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "copyfiles": "^1.2.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}