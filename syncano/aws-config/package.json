{
  "name": "aws-config",
  "version": "0.0.3",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "sync-utils": "if [ -e ../aws-utils ] && [ -e ../merge_deps.js ]; then node ../merge_deps.js aws-utils/package.json; fi",
    "sync-deps": "if [ -e ../aws-utils ] && [ -e node_modules/.bin/sync-files ]; then node_modules/.bin/sync-files ../aws-utils/lib src/aws-utils; fi",
    "preinstall": "npm run sync-utils",
    "postinstall": "npm run sync-deps"
  },
  "dependencies": {
    "syncano-server": "beta",
    "aws-bluebird": "^1.0.0",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "sync-files": "^1.0.3"
  }
}